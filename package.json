  "module": "dist/tokens.js",
  "keywords": [
    "token",
    "autocomplete"
  ],
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prepublishOnly": "scriptkit"
  },
  "repository": "https://github.com/firstandthird/tokens",
  "author": "First+Third",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-firstandthird": "^4.4.0",
    "eslint-plugin-import": "^2.18.0",
    "scriptkit": "^3.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "dependencies": {
    "@firstandthird/complete": "^2.1.0",
    "domassist": "^2.2.0",
