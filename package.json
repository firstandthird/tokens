{
  "name": "tokens",
  "version": "1.0.0",
  "description": "Class based modules",
  "main": "dist/tokens.js",
  "module": "lib/tokens.js",
  "keywords": [
    "token",
    "autocomplete"
  ],
  "scripts": {
    "start": "scriptkit",
    "build": "scriptkit build",
    "dev": "scriptkit dev",
    "test": "cat test/tokens.test.dist.js | tape-run --static . --browser phantomjs --render tap-spec",
    "prepublish": "scriptkit"
  },
  "watch": {
    "lib/*.js": [
      "lint",
      "test"
    ],
    "test/*.js": [
      "lint",
      "test"
    ]
  },
  "repository": "https://github.com/firstandthird/tokens",
  "author": "First+Third",
  "license": "MIT",
  "devDependencies": {
    "eslint-config-firstandthird": "3.2.0",
    "eslint-plugin-import": "2.2.0",
    "phantomjs-prebuilt": "^2.1.14",
    "scriptkit": "0.0.22",
    "tap-spec": "4.1.1",
    "tape-rollup": "4.6.4",
    "tape-run": "2.2.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": "firstandthird"
  },
  "dependencies": {
    "@firstandthird/complete": "1.0.0-beta.11",
    "domassist": "1.6.1",
    "domodule": "5.1.1"
  },
  "scriptkit": {
    "files": {
      "dist/tokens.js": "lib/tokens.js",
      "test/tokens.test.dist.js": "test/tokens.test.js"
    },
    "scripts": {
      "commonjs": {
        "ignore": [
          "node_modules/tape-rollup/*"
        ]
      }
    }
  }
}
